@model List<OnlineRegistration.Models.LogInData>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Stu_Layout.cshtml";
}

<h2>StudentIndex</h2>
<div class="InfoForm">
    @foreach (var usr in Model)
    {
        <div class="Photo">
            <img src="@usr.PhotoByteString" alt="BioPhoto" height="130" width="100">
        </div>
        
        <div class="wraper">
            <div class="left">
                <span>UserName:</span>
            </div>
            <div class="right">
                <span>@usr.UserName</span>
                @*Html.DisplayFor(modelItem => usr.UserName)*@
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>UserType:</span>
            </div>
            <div class="right">
                <span>@usr.UserTypeCode</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>Student Name:</span>
            </div>
            <div class="right">
                <span>@usr.StudentName</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>Id:</span>
            </div>
            <div class="right">
                <span>@usr.StudentCode</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>Department:</span>
            </div>
            <div class="right">
                <span>@usr.DepartmentName</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>AdmissionYear:</span>
            </div>
            <div class="right">
                <span>@usr.AdmissionYear</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>FatherName:</span>
            </div>
            <div class="right">
                <span>@usr.FatherName</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>Mother Name:</span>
            </div>
            <div class="right">
                <span>@usr.MotherName</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>Sex:</span>
            </div>
            <div class="right">
                <span>@usr.Sex</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>DateOfBirth:</span>
            </div>
            <div class="right">
                <span>@usr.DateOfBirth</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>Hall:</span>
            </div>
            <div class="right">
                <span>@usr.Hall</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>Address:</span>
            </div>
            <div class="right">
                <span>@usr.Address</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>BloodGroup:</span>
            </div>
            <div class="right">
                <span>@usr.BloodGroup</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>Email:</span>
            </div>
            <div class="right">
                <span>@usr.Email</span>
            </div>
        </div>
        <div class="wraper">
            <div class="left">
                <span>Contact No.:</span>
            </div>
            <div class="right">
                <span>@usr.ContactNo</span>
            </div>
        </div>
    }
</div>




