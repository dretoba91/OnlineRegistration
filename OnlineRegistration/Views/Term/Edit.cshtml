@model OnlineRegistration.Models.Term

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("Create", "Term", FormMethod.Post, new { @class = "InfoForm" }))
{
   @Html.HiddenFor(x=>x.TermId)
    <div class="wraper">

        <div class="left">
            <span>Term Code:</span>
        </div>
        <div class="right">
            @Html.TextBoxFor(x => x.TermCode, new { @class = "fullwidth" })
        </div>
    </div>
    <div class="wraper">

        <div class="left">

        </div>
        <div class="right">
            <input type="submit" value="Submit" />
        </div>
    </div>
}

