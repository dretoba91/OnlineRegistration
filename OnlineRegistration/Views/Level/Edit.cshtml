@model OnlineRegistration.Models.Level

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("Edit", "Level", FormMethod.Post, new { @class = "InfoForm" }))
{
    @Html.HiddenFor(x=>x.LevelId)
    <div class="wraper">

        <div class="left">
            <span>Level Code:</span>
        </div>
        <div class="right">
            @Html.TextBoxFor(x => x.LevelCode, new { @class = "fullwidth" })
        </div>
    </div>
    <div class="wraper">

        <div class="left">

        </div>
        <div class="right">
            <input type="submit" value="Submit" />
        </div>
    </div>
}

